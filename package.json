{
  "name": "stdnum",
  "version": "1.3.3",
  "description": "Standard Number Validation",
  "files": [
    "lib"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/cjs/index.d.ts",
  "exports_NOT_YET": {
    "./": {
      "import": "./lib/esm/",
      "require": "./lib/cjs/"
    }
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "test": "jest",
    "build": "tsc",
    "prepublishOnly:esm": "tsc -p tsconfig.json",
    "prepublishOnly:cjs": "tsc -p tsconfig-cjs.json",
    "prepublishOnly": "npm run prepublishOnly:esm && npm run prepublishOnly:cjs"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ]
  },
  "author": "David Koblas",
  "keywords": [
    "identity",
    "company",
    "person",
    "vat",
    "validation",
    "validator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/koblas/stdnum-js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-angular": "^16.2.4",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
}
